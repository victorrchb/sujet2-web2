{
  "name": "server",
  "version": "1.0.0",
  "description": "Project Manager API",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "cross-env NODE_ENV=test jest --config jest.config.js --detectOpenHandles --forceExit",
    "test:coverage": "cross-env NODE_ENV=test jest --config jest.config.js --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --config jest.config.js --watch",
    "test:performance": "cross-env NODE_ENV=test jest --config jest.config.js --testMatch=\"**/__tests__/performance.test.js\"",
    "test:all": "npm run test && npm run test:performance",
    "lint": "eslint .",
    "qa": "npm run lint && npm run test:all"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-prom-bundle": "^6.6.0",
    "jsonwebtoken": "^9.0.2",
    "memory-cache": "^0.2.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "prom-client": "^14.2.0",
    "sequelize": "^6.37.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "autocannon": "^7.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
